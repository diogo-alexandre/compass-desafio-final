  "components": {
    "securitySchemes": {
      "BearerAuth": {
        "type": "http",
        "scheme": "bearer"
      }
    },
    "schemas": {
      "Car": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "example": "123"
          },
          "modelo": {
            "type": "string",
            "example": "GM S10 2.8"
          },
          "cor": {
            "type": "string",
            "example": "branco"
          },
          "ano": {
            "type": "string",
            "example": 2021
          },
          "acessorios": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "descricao": {
                  "type": "string",
                  "example": "Ar-condicionado"
                }
              }
            }
          },
          "quantidadePassageiros": {
            "type": "number",
            "example": 5
          }
        }
      },
      "CarEdit": {
        "type": "object",
        "properties": {
          "modelo": {
            "type": "string",
            "example": "GM S10 2.8"
          },
          "cor": {
            "type": "string",
            "example": "branco"
          },
          "ano": {
            "type": "string",
            "example": 2021
          },
          "acessorios": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "descricao": {
                  "type": "string",
                  "example": "Ar-condicionado"
                }
              }
            },
            "minItems": 1
          },
          "quantidadePassageiros": {
            "type": "number",
            "example": 5
          }
        }
      },
      "PeopleEdit": {
        "type": "object",
        "properties": {
          "nome": {
            "type": "string",
            "example": "Joaozinho Ciclano"
          },
          "cpf": {
            "type": "string",
            "example": "123.456.789-00"
          },
          "data_nascimento": {
            "type": "string",
            "example": "23/01/2021"
          },
          "email": {
            "type": "string",
            "example": "joaozinho@gmail.com"
          },
          "senha": {
            "type": "string",
            "example": "123456",
            "minLength": 6
          },
          "habilitado": {
            "type": "string",
            "enum": [
              "sim",
              "nao"
            ]
          }
        }
